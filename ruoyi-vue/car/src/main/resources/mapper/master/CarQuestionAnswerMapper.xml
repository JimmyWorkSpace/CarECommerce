<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.car.mapper.master.CarQuestionAnswerMapper">
    
    <select id="selectMaxShowOrder" resultType="java.lang.Integer">
        select COALESCE(MAX(showOrder), 0) from car_question_answer where delFlag = 0
    </select>
    
</mapper>

